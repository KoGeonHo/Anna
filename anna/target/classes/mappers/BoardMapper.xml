<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="edu.fourman.mapper.boardMapper">

<insert id="boardinsert">
	INSERT INTO board(
			 BoardType
			,Title
			,Content
			,Wdate
			,Location
			,Hit
			,isDal
			,Image1
			,Image2
			,Image3
			,Image4
			,Image5
	)VALUES(
			 #{BoardType}
			,#{Title}
			,#{Content}
			,#{Wdate}
			,#{Location}
			,#{Hit}
			,#{isDel}
			,#{Image1}
			,#{Image2}
			,#{Image3}
			,#{Image4}
			,#{Image5}
	)


</insert>


</mapper>